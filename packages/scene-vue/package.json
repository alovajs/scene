{
  "name": "@alova/scene-vue",
  "version": "1.6.0",
  "description": "scenario vue hooks with alova.js",
  "main": "dist/alova-scene.esm.js",
  "module": "dist/alova-scene.esm.js",
  "types": "typings/index.d.ts",
  "homepage": "https://alova.js.org",
  "type": "module",
  "keywords": [
    "react",
    "hooks",
    "xmlhttprequest",
    "vuejs",
    "typescript",
    "vue",
    "reactjs",
    "svelte",
    "axios",
    "request",
    "fetch-api",
    "vue3",
    "react-hooks",
    "superagent",
    "alova"
  ],
  "jsdelivr": "dist/alova-scene.umd.min.js",
  "unpkg": "dist/alova-scene.umd.min.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "cp:files": "cross-env FW=vue node ../../config/cp-files.js",
    "build:esm": "cross-env NODE_ENV=development EXTENSION=vuehooks rollup -c ../../config/rollup.config.esm.js",
    "build:umd": "cross-env NODE_ENV=development EXTENSION=vuehooks rollup -c ../../config/rollup.config.umd.js",
    "build:umd.min": "cross-env NODE_ENV=production EXTENSION=vuehooks rollup -c ../../config/rollup.config.umd.js",
    "build": "npm run clean && npm run cp:files && npm run build:esm && npm run build:umd && npm run build:umd.min",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "JOU-amjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/alovajs/scene.git"
  },
  "bugs": {
    "url": "https://github.com/alovajs/scene/issues"
  },
  "files": [
    "dist",
    "typings/*.d.ts"
  ]
}
