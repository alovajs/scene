    "semantic-release": "^21.0.6",
    "svelte": "^3.52.0",
    "svelte-jester": "^2.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "vue": "^3.3.4"
