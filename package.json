    "coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "commit": "git-cz && git push",
    "lint": "eslint --ext .ts,.js src/**",
    "lint:fix": "eslint --ext .ts,.js src/** --fix"
  },
  "author": "JOU-amjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/alovajs/scene.git"
  },
