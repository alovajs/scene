{
  "name": "@alova/scene",
  "version": "1.1.0",
  "description": "request strategy with alova.js",
  "homepage": "https://github.com/alovajs/scene",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
